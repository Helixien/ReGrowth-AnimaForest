	<!--============================ Anima Brambles =============================-->
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_AnimaBrambles</defName>
		<label>anima brambles</label>
		<description>A very dense cluster of vines that creeps along themselves and become entangled while competing for sunlight. Any person who tries to walk through them will have to slowly navigate as it almost consciously catches on their clothing and attempts to trip them.</description>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Nutrition>0.2</Nutrition>
		</statBases>
		<pathCost>100</pathCost>
		<graphicData>
			<texPath>Things/Plant/AnimaBrambles</texPath>
			<drawSize>1.4</drawSize>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<ingestible/>
		<plant>
			<fertilityMin>0.01</fertilityMin>
			<fertilitySensitivity>0.0</fertilitySensitivity>
			<growDays>3</growDays>
			<harvestWork>60</harvestWork>
			<maxMeshCount>4</maxMeshCount>
			<leaflessGraphicPath>Things/Plant/Leafless/Brambles_Leafless</leaflessGraphicPath>
			<visualSizeRange>0.7~0.85</visualSizeRange>
			<topWindExposure>0.15</topWindExposure>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>200</wildClusterWeight>
			<wildOrder>1</wildOrder>
			<wildEqualLocalDistribution>false</wildEqualLocalDistribution>
		</plant>
	</ThingDef>
	<!--============================ Anima Bell =============================-->
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_AnimaBell</defName>
		<label>anima bell</label>
		<description>A type of incredibly aggressive flowering vine that grows in areas of high Psychic energy concentrations. Because they absorb nutrients so greedily, many other flowers are unable to grow in the same region. Their striking visual qualities are a defense mechanism to prevent humans from wanting to remove them.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Beauty>4</Beauty>
			<Nutrition>0.18</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/AnimaBell</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<hideAtSnowDepth>0.5</hideAtSnowDepth>
		<ingestible/>
		<plant>
			<fertilitySensitivity>0.0</fertilitySensitivity>
			<growDays>2.5</growDays>
			<fertilityMin>0.05</fertilityMin>
			<maxMeshCount>9</maxMeshCount>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>20</wildClusterWeight>
			<visualSizeRange>0.7~0.8</visualSizeRange>
			<topWindExposure>0.3</topWindExposure>
			<wildOrder>1</wildOrder>
			<purpose>Beauty</purpose>
		</plant>
	</ThingDef>
	<!--============================ Anima Mystic Cap =============================-->
	<ThingDef ParentName="PlantBase">
		<defName>RG_Plant_MysticCap</defName>
		<label>mystic cap</label>
		<description>An uncommon cluster of mushrooms that have adapted to regions of high Psychic energy concentrations. Can be harvested for raw fungus.</description>
		<descriptionHyperlinks>
			<ThingDef>RawFungus</ThingDef>
		</descriptionHyperlinks>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Beauty>2</Beauty>
			<Nutrition>0.35</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/MysticCap</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<neverMultiSelect>false</neverMultiSelect>
		<pathCost>10</pathCost>
		<ingestible/>
		<plant>
			<fertilityMin>0.01</fertilityMin>
			<fertilitySensitivity>0.0</fertilitySensitivity>
			<growDays>10</growDays>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestYield>10</harvestYield>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>RawFungus</harvestedThingDef>
			<maxMeshCount>4</maxMeshCount>
			<neverBlightable>true</neverBlightable>
			<visualSizeRange>0.4~0.7</visualSizeRange>
			<topWindExposure>0</topWindExposure>
			<wildClusterRadius>3</wildClusterRadius>
			<wildClusterWeight>5</wildClusterWeight>
			<mustBeWildToSow>true</mustBeWildToSow>
			<purpose>Food</purpose>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<wildOrder>1</wildOrder>
			<wildEqualLocalDistribution>false</wildEqualLocalDistribution>
		</plant>
	</ThingDef>
	<!--============================ Anima Nima =============================-->
	<ThingDef ParentName="CavePlantBase">
		<defName>RG_Plant_Nima</defName>
		<label>nima</label>
		<description>.</description>
		<statBases>
			<MaxHitPoints>100</MaxHitPoints>
			<Beauty>2</Beauty>
			<BeautyOutdoors>2</BeautyOutdoors>
			<Nutrition>0.5</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Nima</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<selectable>true</selectable>
		<neverMultiSelect>false</neverMultiSelect>
		<pathCost>14</pathCost>
		<ingestible/>
		<plant>
			<fertilityMin>0.01</fertilityMin>
			<fertilitySensitivity>0.15</fertilitySensitivity>
			<growDays>40</growDays>
			<dieIfLeafless>true</dieIfLeafless>
			<harvestYield>20</harvestYield>
			<harvestTag>Standard</harvestTag>
			<harvestedThingDef>RawFungus</harvestedThingDef>
			<maxMeshCount>4</maxMeshCount>
			<neverBlightable>true</neverBlightable>
			<visualSizeRange>0.3~0.5</visualSizeRange>
			<topWindExposure>0.05</topWindExposure>
			<wildClusterRadius>3</wildClusterRadius>
			<wildClusterWeight>5</wildClusterWeight>
			<purpose>Food</purpose>
		</plant>
		<comps>
			<li Class="CompProperties_Glower">
				<glowRadius>4.5</glowRadius>
