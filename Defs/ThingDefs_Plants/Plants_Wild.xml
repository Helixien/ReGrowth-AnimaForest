<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!--============================ Anima Heart Blossom =============================-->
	<ThingDef ParentName="BushBase">
		<defName>RG_Plant_AnimaHeartBlossom</defName>
		<label>anima heart blossom</label>
		<description>texttodo</description>
		<statBases>
			<Beauty>6</Beauty>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/AnimaHeartBlossom</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<drawSize>1</drawSize>
